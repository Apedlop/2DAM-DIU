                <TextField fx:id="direccion" GridPane.columnIndex="1" GridPane.rowIndex="2" />
                <TextField fx:id="ciudad" GridPane.columnIndex="1" GridPane.rowIndex="3" />
                <TextField fx:id="localidad" GridPane.columnIndex="1" GridPane.rowIndex="4" />
                <TextField fx:id="provincia" GridPane.columnIndex="1" GridPane.rowIndex="5" />
            </children>
        </GridPane>
        <HBox layoutX="393.0" layoutY="357.0" AnchorPane.bottomAnchor="10.0" AnchorPane.rightAnchor="15.0">
            <children>
                <Button mnemonicParsing="false" onAction="#botonOk" prefWidth="80.0" text="Vale">
                    <HBox.margin>
                        <Insets right="5.0" />
                    </HBox.margin>
                </Button>
                <Button mnemonicParsing="false" onAction="#botonCancelar" text="Cancelar">
                    <HBox.margin>
                        <Insets right="5.0" />
                    </HBox.margin>
                </Button>
            </children></HBox>
    </children>
